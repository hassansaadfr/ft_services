server {
    listen          5050;
    listen          [::]:5050 ;
    server_name     localhost;

    root            /tmp/wordpress;
    index           index.html index.htm index.php;

    location / {
        try_files $uri $uri/ =404;
    }

    location ~ \.php$ {
        fastcgi_pass      127.0.0.1:9000;
        fastcgi_index     index.php;
        include           fastcgi.conf;
	}
}
